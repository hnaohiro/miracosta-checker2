<div class="box">
  <div class="box-header">
    <h3 class="box-title">Editing Room</h3>
    <div class="pull-right box-tools">
      <%= link_to @room, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' do %>
        <i class="fa fa-trash"></i>
      <% end %>
    </div>
  </div>
  <div class="box-body">
    <%= render 'form', room: @room %> 
  </div>
</div>
